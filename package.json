    "chromatic": "npx chromatic --exit-zero-on-changes",
    "prepare": "husky install",
    "generate:aspida": "openapi2aspida",
    "deploy-build": "rm public/mockServiceWorker.js && vercel build && npx @cloudflare/next-on-pages --skip-build",
    "font:pip": "pip install fonttools[ufo,woff,unicode] zopfli brotli",
    "font:subset": "node ./scripts/subset.js"
  },
